    <bean class="io.pragra.learning.july24springbean.BMW" name="m3">
        <constructor-arg name="model" value="m3"></constructor-arg>
        <constructor-arg name="hp" value="600"></constructor-arg>
        <constructor-arg name="year" value="2022"></constructor-arg>
        <constructor-arg name="engine" ref="dieselEngine"></constructor-arg>
    </bean>

    <bean class="io.pragra.learning.july24springbean.BMW" name="m5">
        <constructor-arg name="model" value="m5"></constructor-arg>
        <constructor-arg name="hp" value="650"></constructor-arg>
        <constructor-arg name="year" value="2024"></constructor-arg>
        <constructor-arg name="engine" ref="petrolEngine"></constructor-arg>
    </bean>
    <bean class="io.pragra.learning.july24springbean.DieselEngine" name="dieselEngine">
        <constructor-arg name="horsePower" value="600"></constructor-arg>
    </bean>
    <bean class="io.pragra.learning.july24springbean.PetrolEngine" name="petrolEngine">
        <constructor-arg name="horsePower" value="650"></constructor-arg>
    </bean>

