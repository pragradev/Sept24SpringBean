<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="io.pragra.learning.july24springbean.Employee" name="employee">
        <constructor-arg name="firstName" value="Asma"></constructor-arg>
        <constructor-arg name="lastName" value="S"></constructor-arg>
        <constructor-arg name="company" value="RBC"></constructor-arg>
    </bean>
</beans>